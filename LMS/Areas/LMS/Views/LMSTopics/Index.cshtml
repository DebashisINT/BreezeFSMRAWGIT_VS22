    function OnClickDeleteLMSPoint(TopicID) {
        var url = '/LMSTopics/TopicDelete/';

        Swal.fire({
            title: 'Confirm Delete?',
            text: "Are you sure you want to delete this topic?",
            icon: 'question',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, delete it!',
            cancelButtonText: 'Cancel'
        }).then((result) => {
            if (result.isConfirmed) {
                $.ajax({
                    type: "POST",
                    url: url,
                    data: JSON.stringify({ 'TopicID': TopicID }),
                    async: false,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                        console.log(response);
                        if (response == "-10") {
                            Swal.fire({
                                icon: 'error',
                                title: 'Error',
                                text: 'Error in Delete.'
                            });
                            //GridShow();
                        }
                        else if (response == "1") {
                            Swal.fire({
                                icon: 'success',
                                title: 'Deleted',
                                text: 'Successfully Deleted.'
                            }).then(() => {
                                gridLMSTopic.Refresh();
                            });
                            //GridShow();
                        }
                        else {
                            Swal.fire({
                                icon: 'info',
                                title: 'Info',
                                text: response
                            });
                        }
                    },
                    error: function (response) {
                        console.log(response);
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: 'An error occurred while deleting the topic.'
                        });
                    }
                });
            }
        });
    }


