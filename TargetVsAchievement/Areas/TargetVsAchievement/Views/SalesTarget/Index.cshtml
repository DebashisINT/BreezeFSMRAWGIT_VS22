    $(document).ready(function () {

        //setTimeout(function () { AddNewRowWithSl(); }, 200);
        var d = new Date();
        SalesTargetDate.SetValue(d);
        var hdnSalesTargetLevel = $('#hdnSalesTargetLevel').val();
        var hdnDetailsID = $('#hdnDetailsID').val();

        if (hdnSalesTargetLevel != '' && hdnDetailsID > 0) {
            $('#ddlTargetLevel').val(hdnSalesTargetLevel);
           // $('#HeaderLableID').text("Sales Target -Modify");
            $('#ddlTargetLevel').prop('disabled', true);
            $('#SalesTargetNo').prop('disabled', true);

            SalesTargetDate.SetEnabled(false);

            var partsDT = $('#hdnSalesTargetDate').val().split("/");
            var dayDT = parseInt(partsDT[1], 10);
            var monthDT = parseInt(partsDT[0], 10) - 1; // Month is 0-based
            var yearDT = parseInt(partsDT[2], 10);

            var dateD = new Date(yearDT, monthDT, dayDT); // Month is 0-based, so 10 represents November
            SalesTargetDate.SetDate(dateD);

        }

        $('#SalesTargetNo').blur(function () {
            var value = $(this).val();
            //alert(value);
            var hdnDetailsID = $("#hdnDetailsID").val();
            $.ajax({
                type: "POST",
                url: "@Url.Action("CHECKUNIQUETARGETDOCNUMBER", "SalesTarget")",
                data: JSON.stringify({ SalesTargetNo: value, TargetID: hdnDetailsID }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    CheckUniqueCode = msg;
                    if (CheckUniqueCode == 1) {
                        //jAlert('Please enter unique No.');
                        Swal.fire({
                            icon: 'warning',
                            title: 'Sorry!',
                            text: 'Please enter unique Document No to proceed.'
                        });
                        $('#SalesTargetNo').val("");
                        $('#SalesTargetNo').Focus();
                    }

                }

            });
        });
    });

