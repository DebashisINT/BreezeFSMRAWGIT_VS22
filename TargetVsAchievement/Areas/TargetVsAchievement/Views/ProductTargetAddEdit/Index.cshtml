<script>

    var globalrowindex = 0;
    var rowtime = 0;
    var savemode = "";
    var hasmsg = 0;

    function CancelProductTargetAdd() {
        window.location.href = '/TargetVsAchievement/TargetSetUp/Index';
    }

    $(document).ready(function () {

        //setTimeout(function () { AddNewRowWithP(); }, 200);
        var d = new Date();
        ProductTargetDate.SetValue(d);
        var hdnSalesTargetLevel = $('#hdnSalesTargetLevel').val();
        var hdnDetailsID = $('#hdnDetailsID').val();

        if (hdnSalesTargetLevel != '' && hdnDetailsID > 0) {
            $('#ddlTargetLevel').val(hdnSalesTargetLevel);
            //$('#HeaderLableID').text("Product Target -Modify");
            $('#ddlTargetLevel').prop('disabled', true);
            $('#ProductTargetNo').prop('disabled', true);

            ProductTargetDate.SetEnabled(false);

            var partsDT = $('#hdnProductTargetDate').val().split("/");
            var dayDT = parseInt(partsDT[1], 10);
            var monthDT = parseInt(partsDT[0], 10) - 1; // Month is 0-based
            var yearDT = parseInt(partsDT[2], 10);

            var dateD = new Date(yearDT, monthDT, dayDT); // Month is 0-based, so 10 represents November
            ProductTargetDate.SetDate(dateD);
        }

        $('#ProductTargetNo').blur(function () {
            var value = $(this).val();
            //alert(value);
            var hdnDetailsID = $("#hdnDetailsID").val();
            $.ajax({
                type: "POST",
                url: "@Url.Action("CHECKUNIQUETARGETDOCNUMBER", "ProductTargetAddEdit")",
                data: JSON.stringify({ ProductTargetNo: value, TargetID: hdnDetailsID }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    CheckUniqueCode = msg;
                    if (CheckUniqueCode == 1) {
                        // jAlert('Please enter unique No.');
                        Swal.fire({
                            icon: 'warning',
                            title: 'Sorry!',
                            text: 'Please enter unique Document No to proceed.'
                        });
                        $('#ProductTargetNo').val("");
                        $('#ProductTargetNo').Focus();
                    }

                }

            });
        });
    });


    // Start Select Region Level
    function OpenLevelList(s, e) {
        if (gridProductTargetEntryList.GetDataRow(globalrowindex) != null) {
            slno = gridProductTargetEntryList.GetDataRow(globalrowindex).children[0].innerHTML.trim();
        }
        var TargetLevel = $("#ddlTargetLevel").val();
        //if (TargetLevel = "Region") {
        //    GridRegionlist("GETREGION");
        //}

        if (TargetLevel != "") {
            GridRegionlist(TargetLevel);
        }

        typemodal = "A";
        $('#GridRegionlistModel').modal('show');
        $('#txtGridLevelName').focus();

    }
    function GridRegionlistkeydown(e) {

        if (e.code == "Enter" || e.code == "NumpadEnter") {
            //if ($("#txtGridLevelName").val() != '') {
            var TargetLevel = $("#ddlTargetLevel").val();
            //if (TargetLevel = "Region") {
            //    GridRegionlist("GETREGION");
            //}
            if (TargetLevel != "") {
                GridRegionlist(TargetLevel);
            }

            //}
        }
        else if (e.code == "ArrowDown") {
            if ($("input[GridTargetIndex=" + 0 + "]")) {
                $("input[GridTargetIndex=" + 0 + "]").focus();
                iindexprod++;
            }
            if ($("input[NonIProductIndex=" + 0 + "]")) {
                $("input[NonIProductIndex=" + 0 + "]").focus();
                iindexprod++;
            }
        }
    }
    function GridRegionlist(Action) {
        var SearchKey = $("#txtGridLevelName").val()
        //GridRegionlist = 1;
        var OtherDetails = {}
        OtherDetails.SearchKey = SearchKey;
        OtherDetails.Type = Action;

        var HeaderCaption = [];
        HeaderCaption.push("Code");
        HeaderCaption.push("Name");

        callonServer("../../../Models/TargetWebService.asmx/GetTargetLevelDetailsList", OtherDetails, "GridTargetTable", HeaderCaption, "GridTargetIndex", "SetGridLevel");

        setTimeout(function () {
            $('#txtGridLevelName').focus();
        }, 600);

    }
    function SetGridLevel(Id, Name, e) {
        //alert(Id)
        // GridRegionlist = 0;
        var TargetLevelID = Id;
        var TargetLevelName = Name;

        if (TargetLevelID != "") {

            var data = TargetLevelID.split('|');
            TargetLevelID = data[0];
            var INTERNALID = data[1];

            btnTARGETLEVEL.SetText(TargetLevelName);
            $("#hdnTARGETLEVELID").val(TargetLevelID);
            $("#hdnINTERNALID").val(INTERNALID);


            $('#GridRegionlistModel').modal('hide');

            $('#ddlTargetLevel').prop('disabled', true);
        }

    }

    function LevelKeyDown(s, e) {
        console.log(e.htmlEvent.key);
        if (e.htmlEvent.key == "Enter") {
            s.OnButtonClick(0);
        }

        else if (e.code == "ArrowDown") {
            if ($("input[GridTargetIndex=0]"))
                $("input[GridTargetIndex=0]").focus();
        }
    }

    // End Select Region Level

    // Start Select Product
    function OpenProductList(s, e) {
        if (gridProductTargetEntryList.GetDataRow(globalrowindex) != null) {
            slno = gridProductTargetEntryList.GetDataRow(globalrowindex).children[0].innerHTML.trim();
        }

        GridProductlist();

        typemodal = "A";
        $('#GridProductlistModel').modal('show');
        $('#txtGridProductName').focus();

    }
    function GridProductlistkeydown(e) {

        if (e.code == "Enter" || e.code == "NumpadEnter") {
            GridProductlist();
        }
        else if (e.code == "ArrowDown") {
            if ($("input[GridTargetIndex=" + 0 + "]")) {
                $("input[GridTargetIndex=" + 0 + "]").focus();
