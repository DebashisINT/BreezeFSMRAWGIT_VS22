    function limitDecimalPlaces(input, decimalPlaces) {
        let value = input.value;
        if (value.includes('.')) {
            let [integerPart, decimalPart] = value.split('.');
            if (decimalPart.length > decimalPlaces) {
                input.value = integerPart + '.' + decimalPart.slice(0, decimalPlaces);
            }
        }
    }
