<script>
    function onShowChooserClick(s, e) {
        UpdateCustomizationWindowVisibility();
    }
    var visibleColumn = [];
    function grid_CustomizationWindowCloseUp(s, e) {
        UpdateButtonText();

        for (var i = 0; i <= gridOutletReVisitDetails.GetColumnsCount() ; i++) {
            if (gridOutletReVisitDetails.GetColumn(i) != null) {
                if (gridOutletReVisitDetails.GetColumn(i).visible == false) {
                    console.log(gridOutletReVisitDetails.GetColumn(i).fieldName);
                    visibleColumn.push(gridOutletReVisitDetails.GetColumn(i).fieldName);
                }
            }
        }

        addPageRetention(visibleColumn);
    }

    function UpdateCustomizationWindowVisibility() {
        if (gridOutletReVisitDetails.IsCustomizationWindowVisible())
            gridOutletReVisitDetails.HideCustomizationWindow();
        else
            gridOutletReVisitDetails.ShowCustomizationWindow();
        UpdateButtonText();
    }

    function UpdateButtonText() {
        var text = gridOutletReVisitDetails.IsCustomizationWindowVisible() ? "Hide" : "Show";
        text += " Column Chooser";
        btShowColumnChooser.SetText(text);
    }

    function addPageRetention(visibleColumn) {
        $.ajax({
            url: "@Url.Action("PageRetention", "OutletRevisitDetails")",
            async: false,
        data: { Columns: visibleColumn },
        type: 'POST',
        success: function (data) {
            gridOutletReVisitDetails.Refresh();
        },
        error: function () {
        },
        });

    }

</script>


