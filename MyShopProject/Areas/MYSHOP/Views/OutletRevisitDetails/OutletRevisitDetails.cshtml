    function LookupValChange() {
    }

    function EmpStartCallback(s, e) {
        e.customArgs["Fromdate"] = dtFrom.GetText();
        e.customArgs["Todate"] = dtTo.GetText();
        e.customArgs["BranchId"] = BranchId;
