<script>
    function onShowChooserClick(s, e) {
        UpdateCustomizationWindowVisibility();
    }
    var visibleColumn = [];
    function grid_CustomizationWindowCloseUp(s, e) {
        UpdateButtonText();

        for (var i = 0; i <= gridDSSummary.GetColumnsCount() ; i++) {
            if (gridDSSummary.GetColumn(i) != null) {
                if (gridDSSummary.GetColumn(i).visible == false) {
                    console.log(gridDSSummary.GetColumn(i).fieldName);
                    visibleColumn.push(gridDSSummary.GetColumn(i).fieldName);
                }
            }
        }

        addPageRetention(visibleColumn);
    }

    function UpdateCustomizationWindowVisibility() {
        if (gridDSSummary.IsCustomizationWindowVisible())
            gridDSSummary.HideCustomizationWindow();
        else
            gridDSSummary.ShowCustomizationWindow();
        UpdateButtonText();
    }

    function UpdateButtonText() {
        var text = gridDSSummary.IsCustomizationWindowVisible() ? "Hide" : "Show";
        text += " Column Chooser";
        btShowColumnChooser.SetText(text);
    }

    function addPageRetention(visibleColumn) {
        $.ajax({
            url: "@Url.Action("PageRetention", "DSSummary")",
            async: false,
        data: { Columns: visibleColumn },
        type: 'POST',
        success: function (data) {
            gridDSSummary.Refresh();
        },
        error: function () {
        },
        });

    }

</script>


