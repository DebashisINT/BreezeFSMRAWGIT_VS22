<script>
    function onShowChooserClick(s, e) {
        UpdateCustomizationWindowVisibility();
    }
    var visibleColumn = [];
    function grid_CustomizationWindowCloseUp(s, e) {
        UpdateButtonText();
        visibleColumn = [];

        for (var i = 0; i <= gridEmployeeActivityStatusList.GetColumnsCount() ; i++) {
            if (gridEmployeeActivityStatusList.GetColumn(i) != null) {
                if (gridEmployeeActivityStatusList.GetColumn(i).visible == false) {
                    console.log(gridEmployeeActivityStatusList.GetColumn(i).fieldName);
                    visibleColumn.push(gridEmployeeActivityStatusList.GetColumn(i).fieldName);
                }
            }
        }

        addPageRetention(visibleColumn);
    }

    function UpdateCustomizationWindowVisibility() {
        if (gridEmployeeActivityStatusList.IsCustomizationWindowVisible())
            gridEmployeeActivityStatusList.HideCustomizationWindow();
        else
            gridEmployeeActivityStatusList.ShowCustomizationWindow();
        UpdateButtonText();
    }

    function UpdateButtonText() {
        var text = gridEmployeeActivityStatusList.IsCustomizationWindowVisible() ? "Hide" : "Show";
        text += " Column Chooser";
        btShowColumnChooser.SetText(text);
    }

    function addPageRetention(visibleColumn) {
        $.ajax({
            url: "@Url.Action("PageRetention", "EmployeeActivityStatus")",
            async: false,
            data: { Columns: visibleColumn },
            type: 'POST',
            success: function (data) {
                gridEmployeeActivityStatusList.Refresh();
            },
            error: function () {
            },
        });

    }

    function gridRowclick(s, e) {
        $('#gridEmployeeActivityStatusList').find('tr').removeClass('rowActive');


        $(s.GetRow(e.visibleIndex)).addClass('rowActive');

    }

</script>

