                empId.push(eids[i]);
            }
        } finally {
            console.log(empId);
        }
    }

    function OpenImage(imgfile) {
        var link = imgfile,
            split = link.split('/'),
            isAdhar = split[4];
        if (isAdhar == 'AadharImage') {
            $('.image').parent('#popupfoot').addClass("adharCard");
        } else {
            $('.image').parent('#popupfoot').removeClass("adharCard");
        }

        $('.image').attr({
            'src': imgfile
        });
        if (imgfile != '') {
            $('#mask').addClass('showing');
        }
        else {
            jAlert("No Image to show");
        }
    }

