@model MyShop.Models.NotificationListClass
@{
    ViewBag.Title = "Notification";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script>
    function OnMoreInfoClick(notificationid,name) {

        var url = "@Url.Action("NotificationSettings", "Notification")" + "?notificationId=" + notificationid + "&notificationame=" + name;

        //window.location.href = "/Notification/NotificationSettings?notificationId=" + notificationid;
        window.location.href = url;


    }

</script>

<div class="breadCumb">
    <span>Notification</span>
</div>

<div class="container mt-5">
    <div class="backBox mt-5 p-3 ">
        <div class="row">
            <div class="col-md-12">
                @Html.Partial("NotificationGrid", Model.notificationList)
            </div>
        </div>
        </div>
    </div>
