        end = end.split('-')[1] + '-' + end.split('-')[0] + '-' + end.split('-')[2];

        var dayse = Math.round((Date.parse(end) - Date.parse(start)) / (1000 * 60 * 60 * 24));

        if (dayse > 35) {
            jAlert("Searching days must be within 35 days");

            return false;
        }

        if ($("#hdnBranchMandatory").val() == "0") {
