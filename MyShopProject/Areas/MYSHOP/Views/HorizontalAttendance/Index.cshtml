        var data = [];
        data.is_pageload = "0";

        var surl = '@Url.Action("PerformanceGridViewCallback", "HorizontalAttendance")';
        // Rev 7.0
        // Making the AJAX call asynchronous
        $.ajax({
            url: surl,
            data: { model: data },
            type: 'POST',
            success: function (data) {
                if (dayse <= 30 && flag == true) {
