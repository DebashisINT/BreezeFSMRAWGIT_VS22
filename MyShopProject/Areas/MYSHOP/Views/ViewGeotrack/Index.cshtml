function hideLoader() {
    $('#gmap-loader').addClass('hide');
}

function showMarkers(data) {
    clearMarkers();

    let totalDistance = 0;

    if (!map) {
        map = new google.maps.Map(document.getElementById("mapid-gmapmarker"), {
            zoom: 6,
            center: { lat: 22.600, lng: 88.431 }, // Set default center
            mapTypeId: google.maps.MapTypeId.ROADMAP,
            scrollwheel: true,
        });
    }

    data.forEach(markerData => {
        addMarkerWithInfo(markerData);

        totalDistance += parseFloat(markerData.Distance_Covered) || 0;
    });

    // Display the total distance covered
    //const totalDistanceText = totalDistance > 0 ? `${totalDistance.toFixed(2)} km` : `0 km`;
    //console.log("Total Distance Covered:", totalDistanceText);

    //document.querySelector('.selected-employee-box h6').textContent = `Total Distance Covered: ${totalDistanceText}`;


    const totalDistanceText = totalDistance.toFixed(2); // Format the total distance to two decimal places
    const totalDistanceSpan = document.querySelector('.selected-employee-box h6 span'); // Select the span element
    totalDistanceSpan.textContent = `${totalDistanceText} km`;

    
    //const salesmanName = data[0].SalesMan;
    //document.querySelector('.selected-employee-box h5').textContent = `User name: ${salesmanName}`;
}

function clearMarkers() {
    markers.forEach(marker => {
        marker.setMap(null);
    });

    markers = [];
}

