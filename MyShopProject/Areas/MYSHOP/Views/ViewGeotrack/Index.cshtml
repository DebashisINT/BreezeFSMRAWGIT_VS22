            return;
        }
        else if ((dtTo.GetDate() - dtFrom.GetDate()) / (1000 * 60 * 60 * 24) > 31) {
            jAlert("Searching days must be within 31 days.");
            flag = false;
            return;
        }
        else {

            $('.FullDiv-heatmap').removeClass('hide');
            showLoader();

            $.ajax({
                type: "POST",
                //url: "Url.Action("GetEmployeesTargetByCode", "EmployeesTarget")",
                url: "@Url.Action("GetUserRoutes", "ViewGeotrack")",
                data: { 'stateid': stateid, 'branchid': branchid, 'empId': empId, 'fromdate': fromdate, 'todate': todate },
                success: function (response) {
                    // Parse JSON response
                    console.log("heatmap", response);
                    var heatmapData = [];
                    response.forEach(function (outlet) {
                        var latLng = new google.maps.LatLng(parseFloat(outlet.Visit_Lat), parseFloat(outlet.Visit_Long));
                        heatmapData.push(latLng);
                    });

                    // Initialize map and heatmap layer
                    var map = new google.maps.Map(document.getElementById("mapid"), {
                        zoom: 13,
                        center: { lat: 22.600, lng: 88.431 },
                        mapTypeId: google.maps.MapTypeId.HYBRID,
                        scrollwheel: true,
                    });

                    var heatmap = new google.maps.visualization.HeatmapLayer({
                        data: heatmapData,
                        map: map,
                    });

                    // Event listeners for the buttons
                    $("#change-gradient").click(function () {
                        const gradient = [
                            "rgba(0, 255, 255, 0)",
                            "rgba(0, 255, 255, 1)",
                            "rgba(0, 191, 255, 1)",
                            "rgba(0, 127, 255, 1)",
                            "rgba(0, 63, 255, 1)",
                            "rgba(0, 0, 255, 1)",
                            "rgba(0, 0, 223, 1)",
                            "rgba(0, 0, 191, 1)",
                            "rgba(0, 0, 159, 1)",
                            "rgba(0, 0, 127, 1)",
                            "rgba(63, 0, 91, 1)",
                            "rgba(127, 0, 63, 1)",
                            "rgba(191, 0, 31, 1)",
                            "rgba(255, 0, 0, 1)",
                        ];

                        heatmap.set("gradient", heatmap.get("gradient") ? null : gradient);
                    });

                    $("#change-opacity").click(function () {
                        heatmap.set("opacity", heatmap.get("opacity") ? null : 0.2);
                    });

                    $("#change-radius").click(function () {
                        heatmap.set("radius", heatmap.get("radius") ? null : 20);
                    });


                    var bounds = new google.maps.LatLngBounds();
                    heatmapData.forEach(function (latLng) {
                        bounds.extend(latLng);
                    });
                    map.fitBounds(bounds);

                    hideLoader();
                },
                error: function (xhr, status, error) {
                    console.error("Error:", error);
                }
            });
        }
    }

    /*heatmap code end*/

    $(document).ready(function () {
        $('#heatmap-container').click(function () {
            $(this).parent('.FullDiv-heatmap').addClass('hide');
        });
    });

    $(document).ready(function () {
        $('#gmapmarker-container').click(function () {
            $(this).parent('.FullDiv-gmapmarker').addClass('hide');
        });
    });


</script>

