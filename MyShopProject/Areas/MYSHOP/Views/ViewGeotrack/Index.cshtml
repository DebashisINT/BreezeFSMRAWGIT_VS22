    function ShowGoogleMapClick() {

        // Rev 1.0
        if (@ViewBag.HasGoogleAPIKey== 0) {
            jAlert("Please configure Google Map Key from System Settings to view map.");
            return;
        }
        // End of Rev 1.0

        //stateid = JSON.stringify(StateId).replaceAll('"', '').replaceAll('[', '').replaceAll(']', '');
        //branchid = JSON.stringify(BranchId).replaceAll('"', '').replaceAll('[', '').replaceAll(']', '');
        const stateid = StateId.join(',');
        const branchid = BranchId.join(',');
        const empId = $('#hdnEmployeeID').val();
        const fromdate = dtFrom.GetText();
        const todate = dtTo.GetText();

        if (stateid == '') {
            jAlert("Please select State.");
            flag = false;
            return;
        }
        else if ($('#hdnEmployeeID').val() == "0" || $('#hdnEmployeeID').val() == "") {
            jAlert("Please select Employee.");
            flag = false;
        }
        else if (fromdate == '') {
            jAlert("Please select From Date.");
            flag = false;
            return;
        }
        else if (todate == '') {
            jAlert("Please select To Date.");
            flag = false;
            return;
        }
        else if ((dtTo.GetDate() - dtFrom.GetDate()) / (1000 * 60 * 60 * 24) > 31) {
            jAlert("Searching days must be within 31 days.");
            flag = false;
            return;
        }
        else {

            $('.FullDiv-gmapmarker').removeClass('hide');

            // Show loader before fetching data
            showLoader();

            // Check if parameters have changed
            if (stateid !== lastStateid || branchid !== lastBranchid || empId !== lastempId || fromdate != lastfromdate || todate != lasttodate ) {
                // If parameters have changed, clear cached data and fetch new data
                cachedData = null;
            }

            if (cachedData) {
                showMarkers(cachedData);
                hideLoader();
            } else {
                fetchData(stateid, branchid, empId, fromdate, todate);
            }

            // Update last parameters
            lastStateid = stateid;
            lastBranchid = branchid;
            lastempId = empId;
            lastfromdate = fromdate;
            lasttodate = todate;
        }
}

    function fetchData(stateid, branchid, empId, fromdate, todate) {
    const data = {
        'stateid': stateid,
        'branchid': branchid,
        'empId': empId,
        'fromdate': fromdate,
        'todate': todate
    };

    const url = "@Url.Action("GetUserRoutes", "ViewGeotrack")";

    fetch(url, {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify(data)
    })
    .then(response => {
        if (!response.ok) {
            throw new Error('Network response was not ok');
        }
        return response.json();
    })
    .then(data => {
        cachedData = data;
        showMarkers(data);
    })
    .catch(error => {
        console.error('There was a problem with the fetch operation:', error);
    })
    .finally(() => {
        hideLoader();
    });
}

function showLoader() {
    $('#gmap-loader').removeClass('hide');
