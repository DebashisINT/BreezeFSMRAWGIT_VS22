                        pcModalTemplateImportLogMode.Show();
                        gridTemplateImportLogList.Refresh();
                        gridTemplateImportLogList.Refresh();

                        //pcModalImportLogViewMode.Hide();
                    }
                }
            });
        }
    }

    function GetServerDateFormat(today) {
        if (today != "" && today != null) {
            var dd = today.getDate();
            var mm = today.getMonth() + 1;
            var yyyy = today.getFullYear();

            if (dd < 10) {
                dd = '0' + dd;
            }
            if (mm < 10) {
                mm = '0' + mm;
            }
            today = yyyy + '-' + mm + '-' + dd;
        }
        else {
            today = "";
        }

        return today;
    }

