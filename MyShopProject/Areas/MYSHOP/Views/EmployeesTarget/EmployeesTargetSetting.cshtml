            flag = false;
        }
        else {
            $('#MandatoryToDate').attr('style', 'display:none');
        }

        if (flag) {
            todate = GetServerDateFormat(new Date(txttoDate.GetDate()));
            fromdate = GetServerDateFormat(new Date(txtfromDate.GetDate()));

            $.ajax({
